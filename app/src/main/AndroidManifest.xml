<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.administrator.cnmar">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.VIBRATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/logo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".LoginActivity" />
        <activity android:name=".MainActivity" />
        <activity android:name=".MaterialStockActivity"
            android:windowSoftInputMode="adjustPan|stateHidden"
            />
        <activity android:name=".MaterialStockDetailActivity" />
        <activity
            android:name=".MaterialInOrderDetailActivity"
            android:windowSoftInputMode="adjustPan|stateHidden" />
        <activity android:name="zxing.activity.CaptureActivity" />
        <activity
            android:name=".MaterialOutOrderDetailActivity"
            android:windowSoftInputMode="adjustPan|stateHidden" />
        <activity
            android:name=".MaterialCheckStockManageActivity"
            android:windowSoftInputMode="adjustPan|stateHidden" />
        <activity android:name=".MaterialCheckStockDetailActivity" />
        <activity android:name=".WelcomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ProductStockActivity"
            android:windowSoftInputMode="adjustPan|stateHidden"
            />
        <activity android:name=".ProductStockDetailActivity" />
        <activity android:name=".ProductCheckStockManageActivity"
            android:windowSoftInputMode="adjustPan|stateHidden"
            />
        <activity android:name=".ProductCheckStockDetailActivity"></activity>
    </application>

</manifest>