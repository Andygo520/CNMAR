<LinearLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        >
        <include layout="@layout/top"></include>
        <TextView
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:text="库存详情"
            android:textColor="@android:color/black"
            android:textStyle="bold"
            android:layout_gravity="top"
            android:gravity="center"
            />
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="5dp"
            android:orientation="vertical"
            >
            <View
                style="@style/list_item_seperator_layout"
                />
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <TableLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:stretchColumns="3,7">
                    <TableRow
                        android:background="@drawable/table_frame"
                        >
                        <View
                            style="@style/list_item_cell_seperator_layout"
                            />
                        <TextView
                            android:text="原料编码"
                            android:textColor="@android:color/black"
                            android:textStyle="bold"
                            android:layout_width="60dip"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:padding="2dip"
                            />
                        <View
                            style="@style/list_item_cell_seperator_layout"
                            />
                        <TextView
                            android:id="@+id/tvMaterialCode"
                            android:layout_width="35dp"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:padding="2dip"
                            />
                        <View
                            style="@style/list_item_cell_seperator_layout"
                            />
                        <TextView
                            android:text="原料名称"
                            android:textColor="@android:color/black"
                            android:textStyle="bold"
                            android:layout_width="60dip"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:padding="2dip"
                            />
                        <View
                            style="@style/list_item_cell_seperator_layout"
                            />
                        <TextView
                            android:id="@+id/tvMaterialName"
                            android:layout_width="35dp"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:padding="2dip"
                            />
                        <View
                            style="@style/list_item_cell_seperator_layout"
                            />
                    </TableRow>
                    <TableRow
                        android:background="@drawable/table_frame"
                        >
                        <View
                            style="@style/list_item_cell_seperator_layout"
                            />
                        <TextView
                            android:text="规格"
                            android:textColor="@android:color/black"
                            android:textStyle="bold"
                            android:layout_width="60dip"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:padding="2dip"
                            />
                        <View
                            style="@style/list_item_cell_seperator_layout"
                            />
                        <TextView
                            android:id="@+id/tvSize"
                            android:layout_width="35dp"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:padding="2dip"
                            />
                        <View
                            style="@style/list_item_cell_seperator_layout"
                            />
                        <TextView
                            android:text="单位"
                            android:textColor="@android:color/black"
                            android:textStyle="bold"
                            android:layout_width="60dip"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:padding="2dip"
                            />
                        <View
                            style="@style/list_item_cell_seperator_layout"
                            />
                        <TextView
                            android:id="@+id/tvUnit"
                            android:layout_width="35dp"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:padding="2dip"
                            />
                        <View
                            style="@style/list_item_cell_seperator_layout"
                            />
                    </TableRow>
                    <TableRow
                        android:background="@drawable/table_frame"
                        >
                        <View
                            style="@style/list_item_cell_seperator_layout"
                            />
                        <TextView
                            android:text="备注"
                            android:textColor="@android:color/black"
                            android:textStyle="bold"
                            android:layout_width="60dip"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:padding="2dip"
                            />
                        <View
                            style="@style/list_item_cell_seperator_layout"
                            />
                        <TextView
                            android:id="@+id/tvRemark"
                            android:layout_width="35dp"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:padding="2dip"
                            />
                        <View
                            style="@style/list_item_cell_seperator_layout"
                            />
                        <TextView
                            android:text="供应商编码"
                            android:textColor="@android:color/black"
                            android:textStyle="bold"
                            android:layout_width="60dip"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:padding="2dip"
                            />
                        <View
                            style="@style/list_item_cell_seperator_layout"
                            />
                        <TextView
                            android:id="@+id/tvSupplierCode"
                            android:layout_width="35dp"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:padding="2dip"
                            />
                        <View
                            style="@style/list_item_cell_seperator_layout"
                            />
                    </TableRow>
                    <TableRow
                        android:background="@drawable/table_frame"
                        >
                        <View
                            style="@style/list_item_cell_seperator_layout"
                            />
                        <TextView
                            android:text="不同批次混仓"
                            android:textColor="@android:color/black"
                            android:textStyle="bold"
                            android:layout_width="60dip"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:padding="2dip"
                            />
                        <View
                            style="@style/list_item_cell_seperator_layout"
                            />
                        <TextView
                            android:id="@+id/tvIsMixed"
                            android:layout_width="35dp"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:padding="2dip"
                            />
                        <View
                            style="@style/list_item_cell_seperator_layout"
                            />
                        <TextView
                            android:text="库存总量"
                            android:textColor="@android:color/black"
                            android:textStyle="bold"
                            android:layout_width="60dip"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:padding="2dip"
                            />
                        <View
                            style="@style/list_item_cell_seperator_layout"
                            />
                        <TextView
                            android:id="@+id/tvStockSum"
                            android:layout_width="35dp"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:padding="2dip"
                            />
                        <View
                            style="@style/list_item_cell_seperator_layout"
                            />
                    </TableRow>
                    <TableRow
                        android:background="@drawable/table_frame"
                        >
                        <View
                            style="@style/list_item_cell_seperator_layout"
                            />
                        <TextView
                            android:text="预警最小库存"
                            android:textColor="@android:color/black"
                            android:textStyle="bold"
                            android:layout_width="60dip"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:padding="2dip"
                            />
                        <View
                            style="@style/list_item_cell_seperator_layout"
                            />
                        <TextView
                            android:id="@+id/tvMinStock"
                            android:layout_width="35dp"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:padding="2dip"
                            />
                        <View
                            style="@style/list_item_cell_seperator_layout"
                            />
                        <TextView
                            android:text="预警最大库存"
                            android:textColor="@android:color/black"
                            android:textStyle="bold"
                            android:layout_width="60dip"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:padding="2dip"
                            />
                        <View
                            style="@style/list_item_cell_seperator_layout"
                            />
                        <TextView
                            android:id="@+id/tvMaxStock"
                            android:layout_width="35dp"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:padding="2dip"
                            />
                        <View
                            style="@style/list_item_cell_seperator_layout"
                            />
                    </TableRow>
                </TableLayout>
            </LinearLayout>
            <View
                style="@style/list_item_seperator_layout"
                />
        </LinearLayout>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:text="仓位信息"
            android:textColor="@android:color/black"
            android:textStyle="bold"
            android:gravity="center"
            />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="5dp"
            >
            <View
                style="@style/list_item_seperator_layout"
                />
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <TableLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:stretchColumns="1,3,5,7">
                    <TableRow
                        android:id="@+id/stock_list_header_row">
                        <View
                            style="@style/list_item_cell_seperator_layout"
                            />
                        <TextView
                            android:id="@+id/stock_list_header_code"
                            android:text="仓位编码"
                            android:textColor="@android:color/black"
                            android:textStyle="bold"
                            android:layout_width="60dip"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:padding="2dip"
                            />
                        <View
                            style="@style/list_item_cell_seperator_layout"
                            />
                        <TextView
                            android:id="@+id/stock_list_header_symbol"
                            android:text="仓位名称"
                            android:textColor="@android:color/black"
                            android:textStyle="bold"
                            android:layout_width="60dip"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:padding="2dip"
                            />
                        <View
                            style="@style/list_item_cell_seperator_layout"
                            />
                        <TextView
                            android:id="@+id/stock_list_header_last_price"
                            android:text="仓位容量"
                            android:textColor="@android:color/black"
                            android:textStyle="bold"
                            android:layout_width="60dip"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:padding="2dip"
                            />
                        <View
                            style="@style/list_item_cell_seperator_layout"
                            />
                        <TextView
                            android:id="@+id/stock_list_header_price_change"
                            android:text="库存数量"
                            android:textColor="@android:color/black"
                            android:textStyle="bold"
                            android:layout_width="60dip"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:padding="2dip"
                            />
                        <View
                            style="@style/list_item_cell_seperator_layout"
                            />
                    </TableRow>
                </TableLayout>
            </LinearLayout>
            <View
                style="@style/list_item_seperator_layout"
                />
            <ListView
                android:id="@+id/lvSpace"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:scrollingCache="true"
                android:fastScrollEnabled="true"
                android:focusable="true"
                android:divider="@color/color_dark_grey"
                android:dividerHeight="1dip"
                />
        </LinearLayout>

    </LinearLayout>
